{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": 0,
    "no-unused-vars": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "no-useless-constructor": 1,
    "no-empty-function": 1
  }
}
